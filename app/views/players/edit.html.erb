<h1>Edit Player Profile</h1>
<%= form_for @player do |f| %>

<%= f.label :email %>
<%= f.email_field :email %>

<%= f.label :name %>
<%= f.text_field :name %>

<%= f.label :password %>
<%= f.password_field :password %>

<%= f.label :password_confirmation %>
<%= f.password_field :password_confirmation %>

<%= f.label :DOB %>
<%= f.text_field :dob %>

<%= f.submit %>
<% end %>
